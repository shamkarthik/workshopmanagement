@model IEnumerable<Workshop_Application.Models.WorkshopParticipant>
@{
    ViewBag.Title = "WorkshopParticipant";
}

<h2>WorkshopParticipant</h2>
<table>
    @foreach (var item in Model)
    {
    <tr>
        <td>@Html.DisplayFor(modelitem => item.Username)</td>
        <td>@Html.DisplayFor(modelitem => item.PhoneNumber)</td>
        <td>@Html.DisplayFor(modelitem => item.Email)</td>
        <td>@Html.DisplayFor(modelitem => item.WorkshopId)</td>
        <td>@Html.DisplayFor(modelitem => item.ParticipantAttended)</td>
    </tr>
    }
</table>